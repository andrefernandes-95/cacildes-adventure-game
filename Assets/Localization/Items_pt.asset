    m_Metadata:
      m_Items: []
  - m_Id: 24713822894686208
    m_Localized: "Botas do Centuri\xE3o"
    m_Metadata:
      m_Items: []
  - m_Id: 24714264768806912
    m_Localized: "Manoplas do Centuri\xE3o"
    m_Metadata:
      m_Items: []
  - m_Id: 24771346436063232
