    m_Metadata:
      m_Items: []
  - m_Id: 1793429867462656
    m_Localized: Botas de Esmeralda
    m_Metadata:
      m_Items: []
  - m_Id: 1793562302611456
    m_Localized: Botas Douradas
    m_Metadata:
      m_Items: []
  - m_Id: 1793753273466880
    m_Localized: Botas do Guerreiro Dourado
    m_Metadata:
      m_Items: []
  - m_Id: 1793953350156288
    m_Localized: Botas de Ferro
    m_Metadata:
      m_Items: []
  - m_Id: 1794230111305728
    m_Localized: Botas de Cavaleiro
    m_Metadata:
      m_Items: []
  - m_Id: 1794301309616128
    m_Localized: Botas de Couro
